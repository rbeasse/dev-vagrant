#
# site: dev.ryanbeasse.com
server {
  listen 80;
  listen [::]:80;

  server_name dev.ryanbeasse.com;

  root /projects/ryanbeasse/_site;
  index index.html;

  location / {
    try_files $uri $uri/ =404;
  }
}


